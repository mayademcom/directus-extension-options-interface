{
  "name": "@mayademcom/directus-extension-options-interface",
  "description": "A sophisticated repeater interface for Directus CMS that enables creation of multiple choice questions with drag & drop reordering, rich content editing, and flexible validation rules. Perfect for quizzes, surveys, and educational content management.",
  "icon": "extension",
  "version": "1.1.0",
  "author": "Mayadem",
  "license": "MIT",
  "keywords": [
    "directus",
    "extension",
    "interface",
    "multiple-choice",
    "quiz",
    "questions"
  ],
  "readmeFilename": "README.md",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mayademcom/directus-extension-options-interface"
  },
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "interface",
    "path": "dist/index.js",
    "source": "src/index.js",
    "host": "^10.0.0",
    "hidden": false
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "validate": "directus-extension validate",
    "release": "release-it"
  },
  "dependencies": {
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^13.0.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "conventional-changelog-cli": "^5.0.0",
    "release-it": "^19.0.1"
  }
}
